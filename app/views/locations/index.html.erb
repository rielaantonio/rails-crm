<h1>Listing locations</h1>

<table>
  <tr>
    <th>Line1</th>
    <th>Line2</th>
    <th>City</th>
    <th>State</th>
    <th>Zipcode</th>
    <th>County</th>
    <th>Country</th>
  </tr>

<% for location in @locations %>
  <tr>
    <td><%=h location.line1 %></td>
    <td><%=h location.line2 %></td>
    <td><%=h location.city_id %></td>
    <td><%=h location.state_id %></td>
    <td><%=h location.zipcode_id %></td>
    <td><%=h location.county_id %></td>
    <td><%=h location.country_id %></td>
    <td><%= link_to 'Show', location %></td>
    <td><%= link_to 'Edit', edit_location_path(location) %></td>
    <td><%= link_to 'Destroy', location, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New location', new_location_path %>
